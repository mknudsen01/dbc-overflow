<!DOCTYPE html>
<html>
<head>
  <title>DbcOverflow</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="navbar">
  <ul>
    <li><%= link_to("home", questions_path) %></li>
    <% if !!session[:id] %>
      <li><%= link_to 'Logout', session_path, :method => 'delete' %></li>
    <% else %>
      <li><%= link_to 'Login', users_path %></li>
      <li><%= link_to 'Signup', new_user_path %></li>
    <% end %>
  </ul>
</div>


<%= yield %>

</body>
</html>
