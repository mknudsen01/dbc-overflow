<h1>Ask a question</h1>
<%= form_for @question, url: { action: "create" }, html: {class: "question_form"} do |f|%>
  <%= f.label :question_subject %><br>
  <%= f.text_area :title, size: "60x1" %><br>
  <%= f.label :question %><br>
  <%= f.text_area :body, size: "60x18" %><br>
  <%= f.submit "Create" %>
<% end %>